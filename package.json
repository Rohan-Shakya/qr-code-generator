{
  "name": "vue-qr-code",
  "version": "0.1.0",
  "description": "A Vue component for QR code.",
  "type": "module",
  "main": "./dist/vue-qr-code.vue.cjs.js",
  "module": "./dist/vue-qr-code.vue.esm.js",
  "browser": "./dist/vue-qr-code.vue.browser.js",
  "unpkg": "./dist/vue-qr-code.vue.browser.min.js",
  "jsdelivr": "./dist/vue-qr-code.vue.browser.min.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-qr-code.vue.esm.js",
      "require": "./dist/vue-qr-code.vue.cjs.js"
    },
    "./package": "./package.json"
  },
  "scripts": {
    "lint": "yarn eslint .",
    "format": "prettier --write --ignore-unknown '**/*.{js,ts,mjs,cjs,json}'",
    "format:ci": "prettier --check --ignore-unknown '**/*.{js,ts,mjs,cjs,json}'",
    "dev": "webpack serve --config webpack.config.js --mode=development",
    "build": "rollup -c",
    "start:server": "node demo/server.js",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rohan-Shakya/vue-qr-code.git"
  },
  "keywords": [
    "vue-qr-code",
    "qrcode",
    "vue-qr",
    "vue"
  ],
  "files": [
    "/dist",
    "README.md"
  ],
  "author": "Rohan-Shakya <contact@shakyarohan.com.np>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rohan-Shakya/vue-qr-code/issues"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "homepage": "https://github.com/Rohan-Shakya/vue-qr-code#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vue/compiler-sfc": "^3.3.11",
    "@vue/server-renderer": "^3.3.11",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^9.1.0",
    "html-webpack-plugin": "^5.5.4",
    "husky": "^9.0.11",
    "prettier": "^3.0.3",
    "rollup": "^4.8.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "vue": "^3.3.11",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
